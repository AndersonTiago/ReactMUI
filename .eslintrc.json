{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "global-require": 0,
      "import/no-dynamic-require": 0,
      "camelcase": [
        "error",
        {
          "properties": "never",
          "ignoreDestructuring": true
        }
      ]
    }
  }